#JSGF V1.0;

/**
 * JSGF Grammar for Turtle example
 */

grammar 1chiffre;

public <number> = <infini>;

public <a1> = <distance>;

public <a3> = <distance> <distance> <distance>;

public <a5> = <distance> <distance> <distance> <distance> <distance>;

<infini> = <distance> <infini> | <distance>;

<distance> = one | two | three | four | five | six | seven | eight | nine | ten ;




